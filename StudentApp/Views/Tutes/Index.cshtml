@model IEnumerable<StudentApp.Models.Tutes>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Tute_title)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Sub_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Teacher_name_)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Tute_title)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Sub_name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Teacher_name_)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Tute_id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Tute_id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Tute_id })
        </td>
    </tr>
}

</table>
